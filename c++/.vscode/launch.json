{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "chrome",
          "request": "launch",
          "name": "Debug WebAssembly",
          "url": "http://localhost:8000",
          "webRoot": "${workspaceFolder}",
          "sourceMaps": true,
          "sourceMapPathOverrides": {
              "webpack:///*": "${webRoot}/*"
          },
          "preLaunchTask": "serve"
      }
  ]
}